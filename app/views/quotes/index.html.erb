  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      
            <th>Id product</th>
      
            <th>Price</th>
      
            <th>Margin</th>
      
            <th>Quantity</th>
      
            <th>Net price</th>
      
            <th>Cost product</th>
      
            <th>Total price</th>
      
      <th>Fecha Creaci√≥n</th>
    </tr>
  </thead>
  <tbody id="container_quotes">
    <%= render @quotes %>
